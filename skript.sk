command /allowplayers:
	trigger:
		set {whitelisted} to false
		broadcast "&2Minecraft &cQuest &fPlayers are now allowed to join the server."




command /gui:
	trigger:
		set {_gui} to a new chest inventory with 1 row with name "&2Minecraft &cQuest &fSelect your team"
		set slot 0 of {_gui} to red wool named "§cNe§fder§9land" with lore "Click to join!"
		set slot 8 of {_gui} to blue wool named "§9Sve§erige" with lore "Click to join!"
		
		open {_gui} to player
 
on inventory click:
	if name of event-inventory is "&2Minecraft &cQuest &fSelect your team":
		if index of event-slot = 0:
			cancel event
			close player's inventory
			make player execute command "netherlands"
		if index of event-slot = 8:
			cancel event
			close player's inventory
			make player execute command "sweden"
	if name of event-inventory is "&2Minecraft &cQuest &f| §cNe§fder§9land":
		cancel event
		if index of event-slot = 23:
			{%player%.rank} is "mod"
			add 1 to {netherlands.count}
			play sound "block.note_block.pling" at volume 1 at pitch 2 to player
			broadcast "&2Minecraft &cQuest | &f+1 Point for The Netherlands. They now have %{netherlands.count}% points!" 

		if index of event-slot = 21:
			{%player%.rank} is "mod"
			remove 1 from {netherlands.count}
			play sound "block.note_block.pling" at volume 1 at pitch 0 to player
			if {netherlands.count} is less than 0:
				set {netherlands.count} to 0
			broadcast "&2Minecraft &cQuest | &f-1 Point for The Netherlands. They now have %{netherlands.count}% points!" 

		set slot 4 of current inventory to red stained glass pane named "§cCurrent points" with lore "%{netherlands.count}%"
	
	
	if name of event-inventory is "&2Minecraft &cQuest &f| §9Sve§erige":
		cancel event
		if index of event-slot = 23:
			{%player%.rank} is "mod"
			add 1 to {sweden.count}
			play sound "block.note_block.pling" at volume 1 at pitch 2 to player
			broadcast "&2Minecraft &cQuest | &f+1 Point for Sweden. They now have %{sweden.count}% points!" 

		if index of event-slot = 21:
			{%player%.rank} is "mod"
			remove 1 from {sweden.count}
			play sound "block.note_block.pling" at volume 1 at pitch 0 to player
			if {sweden.count} is less than 0:
				set {sweden.count} to 0
			broadcast "&2Minecraft &cQuest | &f-1 Point for Sweden. They now have %{sweden.count}% points!" 

		set slot 4 of current inventory to red stained glass pane named "§cCurrent points" with lore "%{sweden.count}%"

on join:
	create bossbar titled "&2Minecraft &cQuest | &fIn-game" and id "bossbar" for all players with progress 100 with colors blue
every 10 tick:
	{status} is "postgame"
	execute console command "worldborder set 100"
	set health of all players to 10
	loop all players:
		set loop-player's food level to 20

every tick:
	loop all players:
		if {%loop-player%.rank} is "mod":
			set slot 7 of loop-player to red wool named "§cNe§fder§9land" with lore "Add or remove points"
			set slot 8 of loop-player to blue wool named "§9Sve§erige" with lore "Add or remove points"
			if {status} is "waiting":
				set slot 0 of loop-player to green wool named "&eStart" with lore "Start the game"
			else:
				set slot 0 of loop-player to air

on right click with red wool:
	if {%player%.rank} is "mod":
		cancel event
		set {_gui} to a new chest inventory with 3 row with name "&2Minecraft &cQuest &f| §cNe§fder§9land"
		set slot 4 of {_gui} to red stained glass pane named "§cCurrent points" with lore "%{netherlands.count}%"
		set slot 23 of {_gui} to green wool named "Add point" with lore "Click me!"
		set slot 21 of {_gui} to red wool named "Remove point" with lore "Click me!"
		
		open {_gui} to player		
on right click with blue wool:
	if {%player%.rank} is "mod":
		cancel event
		set {_gui} to a new chest inventory with 3 row with name "&2Minecraft &cQuest &f| §9Sve§erige"
		set slot 4 of {_gui} to red stained glass pane named "§cCurrent points" with lore "%{sweden.count}%"
		set slot 23 of {_gui} to green wool named "Add point" with lore "Click me!"
		set slot 21 of {_gui} to red wool named "Remove point" with lore "Click me!"
		
		open {_gui} to player		
on right click with green wool:
	{status} is "waiting"
	{clickedstart} is not true
	{%player%.rank} is "mod"
	execute console command "start"
	set slot 0 of player to air
