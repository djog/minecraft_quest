	trigger:
		send "&2Minecraft &cQuest &f/say is not supported. Please re-type your message without /say."
on world change:
	player's world is not "world"
	player's world is not "world_nether"
	execute console command "mvtp %player% world"

on join:
	player's world is not "world"
	player's world is not "world_nether"
	execute console command "mvtp %player% world"

command /shrink:
	trigger:
		executor is console
		execute console command "worldborder center 0 0"
		execute console command "worldborder set 100 0"
		set {specOnDeath} to true
		set {survivalPlayers} to 0
		loop all players:
			set {_xs} to a random integer between -49 and 50
			set {_zs} to a random integer between -49 and 50
			set {%loop-player%.fallen} to false
			execute console command "tp %loop-player% %{_xs}% 100 %{_zs}%"	
		execute console command "worldborder set 50 60"
		{ended} is false
		wait 30 seconds
		execute console command "worldborder set 40 60"
		{ended} is false
		wait 30 seconds
		execute console command "worldborder set 10 60"
		{ended} is false

		wait 30 seconds
		{ended} is false

		execute console command "worldborder set 5 30"
		

on death of player:
	{specOnDeath} is true
	cancel event
	wait 1 tick
	set gamemode of player to spectator


every second:
	{status} is "fight"
	set {survivalPlayers} to 0
	set {aliveNetherlands} to 0
	set {aliveSweden} to 0
	
	
	{specOnDeath} is true
	loop all players:
		if gamemode of loop-player is survival or adventure:
			add 1 to {survivalPlayers}
			set {winner} to name of loop-player
			set {winner.team} to {%loop-player%.rank}
			if {%loop-player%.rank} is "netherlands":
				add 1 to {aliveNetherlands}
			if {%loop-player%.rank} is "sweden":
				add 1 to {aliveSweden}
			
	if {aliveSweden} is 0:
		{ended} is false
		set {specOnDeath} to false
		set {ended} to true
		broadcast "&2Minecraft &cQuest &f+5 Points for The Netherlands"
		send all players title "&2Minecraft &cQuest" with subtitle "&f+5 Points for The Netherlands" for 3 seconds

		execute console command "playsound minecraft:music_disc.mellohi master @a 0 0 0 1 1 1"
		add 5 to {netherlands.count}
		wait 3 seconds
		broadcast "&2Minecraft &cQuest | &fAnd the winner is..."
		send all players title "&2Minecraft &cQuest" with subtitle "&fAnd the winner is...." for 3 seconds

		wait 5 seconds
		if {netherlands.count} is higher than {sweden.count}:
			broadcast "&2Minecraft &cQuest | &fThe Netherlands!" 
			send all players title "&2Minecraft &cQuest" with subtitle "&fThe Netherlands!" for 10 seconds

		else:
			if {netherlands.count} is equal to {sweden.count}:
				send all players title "&2Minecraft &cQuest" with subtitle "&fIts a tie!" for 10 seconds

			else: 
				broadcast "&2Minecraft &cQuest | &fSweden!" 
				send all players title "&2Minecraft &cQuest" with subtitle "&fSweden!" for 10 seconds
		execute console command "worldborder set 200 0"
		loop all players:
			if gamemode of loop-player is survival or adventure:
				clear inventory of all players
				set gamemode of loop-player to adventure
				execute console command "tp @a %loop-player%"
				remove bossbar "bossbar"
				wait 1 tick
				create bossbar titled "&2Minecraft &cQuest | &fPost-game" and id "bossbar" for all players with progress 100 with colors blue
				set {status} to "postgame"
		wait 12 seconds				
		broadcast "&2Minecraft &cQuest | &fThank you for playing!"
		send all players title "&2Minecraft &cQuest" with subtitle "&fThank you for playing!" for 4 seconds

		wait 5 seconds
		wait 4 seconds
		set {nomove} to true

		apply blindness 10 to all players for 40 seconds
		execute console command "playsound minecraft:music_disc.mellohi master @a 0 0 0 1 1 1"

		send all players title "&2Moderation &cTeam" with subtitle "&fDaan" for 2 seconds
		wait 3 seconds
		send all players title "&2Moderation &cTeam" with subtitle "&fChristiaan" for 2 seconds
		wait 3 seconds
		send all players title "&2Moderation &cTeam" with subtitle "&fRichel" for 2 seconds
		wait 3 seconds
		send all players title "&2Programming &f& &cServer Hosting" with subtitle "&fDaan" for 2 seconds
		wait 3 seconds
		send all players title "&2Organizing GitHub &f& &cstuff" with subtitle "&fRichel" for 2 seconds
		wait 3 seconds
		send all players title "&2Testing &f& &cplaying portal" with subtitle "&fJoshua" for 2 seconds
		wait 3 seconds
		send all players title "&2Testing &fand&c other stuff" with subtitle "&fRijk" for 2 seconds
		wait 3 seconds
		send all players title "&2Testing &fand&c suggesting stuff" with subtitle "&fLeonid" for 2 seconds
		wait 3 seconds		
		send all players title "&2wow'ing &fmy &ccode" with subtitle "&fRohan" for 2 seconds
		wait 3 seconds
		send all players title "&2And lastly..." with subtitle "?" for 5 seconds
		wait 6 seconds
		send all players title "&2You" with subtitle "&ffor playing :)" for 10 seconds
		set {nomove} to false
		
		
		
	if {aliveNetherlands} is 0:
		{ended} is false
		set {specOnDeath} to false
		set {ended} to true
		broadcast "&2Minecraft &cQuest &f+5 Points for Sweden"
		send all players title "&2Minecraft &cQuest" with subtitle "&f+5 Points for Sweden" for 3 seconds

		execute console command "playsound minecraft:music_disc.mellohi master @a 0 0 0 1 1 1"
		add 5 to {sweden.count}
		wait 3 seconds
		broadcast "&2Minecraft &cQuest | &fAnd the winner is..."
		send all players title "&2Minecraft &cQuest" with subtitle "&fAnd the winner is...." for 3 seconds

		wait 5 seconds
		if {netherlands.count} is higher than {sweden.count}:
			broadcast "&2Minecraft &cQuest | &fThe Netherlands!" 
			send all players title "&fThe Netherlands!" for 10 seconds

		else:
			if {netherlands.count} is equal to {sweden.count}:
				broadcast "&2Minecraft &cQuest | &fTie!" 
				send all players title "&fIts a tie!" for 10 seconds

			else:

				broadcast "&2Minecraft &cQuest | &fSweden!" 
				send all players title "&fSweden!" for 10 seconds

		execute console command "worldborder set 200 0"
		loop all players:
			if gamemode of loop-player is survival:
				clear inventory of all players
				set gamemode of loop-player to adventure
				execute console command "tp @a %loop-player%"
				remove bossbar "bossbar"
				create bossbar titled "&2Minecraft &cQuest | &fPost-game" and id "bossbar" for all players with progress 100 with colors blue
				set {status} to "postgame"
		wait 12 seconds				
		broadcast "&2Minecraft &cQuest | &fThank you for playing!"
		send all players title "&2Minecraft &cQuest" with subtitle "&fThank you for playing!" for 3 seconds
		wait 5 seconds
		wait 4 seconds
		set {nomove} to true

		apply blindness 10 to all players for 40 seconds
		execute console command "playsound minecraft:music_disc.mellohi master @a 0 0 0 1 1 1"

		send all players title "&2Moderation &cTeam" with subtitle "&fDaan" for 2 seconds
		wait 3 seconds
		send all players title "&2Moderation &cTeam" with subtitle "&fChristiaan" for 2 seconds
		wait 3 seconds
		send all players title "&2Moderation &cTeam" with subtitle "&fRichel" for 2 seconds
		wait 3 seconds
